#: eel/eel-glib-extensions.c:1261
msgid "1/1/00, 1:00 AM"
msgstr "1/1/00, 1:00 PD"

#. The third one is "%_m/%_d/%_y, %_I:%M %p".
#: eel/eel-glib-extensions.c:1263
msgid " 1/ 1/00,  1:00 AM"
msgstr " 1/ 1/00,  1:00 PD"

