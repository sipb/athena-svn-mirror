    \else
       \ifx\\#1\\\relax
  \literalps@out{[\the\pdf@toks\space \pdf@type\space pdfmark}%
       \else
         \hy@colorlink{\@ifundefined{@\pdf@linktype color}%
                      {\@linkcolor}%
                      {\csname @\pdf@linktype color\endcsname}}%
         \pdf@rect{#1}%
  \literalps@out{[\the\pdf@toks\space \pdf@type\space pdfmark}%
         \hy@endcolorlink
       \fi
    \fi
}
