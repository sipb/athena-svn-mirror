    \put(\l@youtxcii,\l@youtyciii){title end}     % end of title heading
    \put(\l@youthdo,\l@youtyciii){\makebox(0,0)[br]{487}} % page number
    \settowidth{\l@ylen}{title end\space}
    \l@yltoc{\l@ylen}{\l@youthdv}              % width of `title end' text
      \l@youthpvi=\l@youtunitlength
      \multiply\l@youthdv by \onepoint\relax
