\newcommand{\drawpage}{%
  \l@youtvdiii=\teninch\relax
    \divide\l@youtvdiii by 24\relax
  \l@youthdii=\l@youtvdiii
  \ifdrawparameters
    \l@youtph=\eleveninch\relax     % page height
    \l@youtpw=\eighthalfinch\relax  % page width
    \l@youtvpi=\z@                  % voffset
    \l@youtvpii=\l@youtvdiii        % topmargin
    \l@youtvpiii=\l@youtvdiii       % headheight
    \l@youtvpiv=\l@youtvdiii        % headsep
    \l@youtvpv=\oneinch\relax       % textheight
      \multiply\l@youtvpv by 6\relax
    \l@youtvpvi=\l@youtvdiii        % footskip
      \multiply\l@youtvpvi by \tw@
    \l@youtvpvii=\l@youtvdiii       % marginparpush
    \l@youthpi=\z@            % hoffset
    \l@youthpii=\l@youthdii   % textwidth
      \multiply\l@youthpii by 13\relax
    \l@youthpiii=\l@youthdii  % columnsep
    \l@youthpiv=\l@youthdii   % evensidemargin
    \l@youthpv=\l@youthdii    % oddsidemargin
    \l@youthpvi=\l@youthdii   % marginparsep
    \l@youthpvii=\l@youthdii  % marginparwidth
      \multiply\l@youthpvii by \tw@
  \fi
  \l@youtycvi=\l@youtph                  % one inch below top of page
    \advance\l@youtycvi by -\oneinch\relax
  \l@youtxci=\l@youthpi                  % hofref
    \advance\l@youtxci by \oneinch\relax
  \l@youtycv=\l@youtycvi                 % vofref
    \advance\l@youtycv by -\l@youtvpi
  \l@youtyciv=\l@youtycv                 % headref
    \advance\l@youtyciv by -\l@youtvpii
    \advance\l@youtyciv by -\l@youtvpiii
  \l@youtycii=\l@youtyciv                % bodyref
    \advance\l@youtycii by -\l@youtvpiv
    \advance\l@youtycii by -\l@youtvpv
  \l@youtvdi=\l@youtvpvi                 % footheight
    \divide\l@youtvdi by \tw@
  \l@youtyci=\l@youtycii                 % footref
    \advance\l@youtyci by -\l@youtvpvi
  \l@youtvdii=\l@youtvpv                 % noteheight
    \divide\l@youtvdii by 4\relax
  \l@youtyciii=\l@youtycii               % noteref
    \advance\l@youtyciii by \l@youtvdii
    \advance\l@youtyciii by \l@youtvpvii
  \ifoddpagelayout
    \l@youtxcii=\l@youtxci               % marginref
      \advance\l@youtxcii by \l@youthpv
    \l@youtxciv=\l@youtxcii              % margnoteref
      \advance\l@youtxciv by \l@youthpii
      \advance\l@youtxciv by \l@youthpvi
  \else
    \l@youtxcii=\l@youtxci               % marginref
      \advance\l@youtxcii by \l@youthpiv
    \l@youtxciv=\l@youtxcii              % margnoteref
      \advance\l@youtxciv by -\l@youthpvi
      \advance\l@youtxciv by -\l@youthpvii
  \fi
  \l@youthdi=\l@youthpii                 % columnwidth
  \l@youtxciii=\l@youtxcii               % colref
  \iftwocolumnlayout
    \advance\l@youthdi by -\l@youthpiii
    \divide\l@youthdi by \tw@
    \advance\l@youtxciii by \l@youthdi
    \advance\l@youtxciii by \l@youthpiii
    \l@youtxcv=\l@youthpiii              % centre of gutter
      \divide\l@youtxcv by \tw@
      \advance\l@youtxcv by \l@youtxcii
      \advance\l@youtxcv by \l@youthdi
  \fi
  \begin{center}
  \begin{small} Dashed lines represent (\texttt{\bs hoffset + 1 inch}) and
    (\texttt{\bs voffset + 1 inch}) from the top left hand corner of the
    page. \end{small} \\
  \medskip
  \setlength{\unitlength}{\l@youtunitlength}
  \begin{picture}(\l@youtpw,\l@youtph)
    \thicklines
    \ifdrawparameters
      \put(0,0){\line(0,1){\l@youtph}}
      \put(0,\l@youtph){\line(1,0){\l@youtpw}}
    \else
      \put(0,0){\framebox(\l@youtpw,\l@youtph){}}
    \fi
    \put(\oneinch,\l@youtycvi){\circle{12}}
    \put(0,\l@youtycv){\dashbox{10}(\l@youtpw,0){}}
    \put(\l@youtxci,0){\dashbox{10}(0,\l@youtph){}}
    \put(\l@youtxcii,\l@youtyciv){\framebox(\l@youthpii,\l@youtvpiii)%
      {\footnotesize Header}}
    \iftwocolumnlayout
      \put(\l@youtxcii,\l@youtycii){\framebox(\l@youthdi,\l@youtvpv)%
        {Col. 1}}
      \put(\l@youtxciii,\l@youtycii){\framebox(\l@youthdi,\l@youtvpv)%
        {Col. 2}}
      \linethickness{\l@youtlinethick}
      \put(\l@youtxcv,\l@youtycii){\line(0,1){\l@youtvpv}}
      \thicklines
    \else
      \put(\l@youtxcii,\l@youtycii){\framebox(\l@youthpii,\l@youtvpv)%
        {Body}}
    \fi
    \put(\l@youtxcii,\l@youtyci){\framebox(\l@youthpii,\l@youtvdi)%
        {\footnotesize Footer}}
    \ifdrawparameters
      \put(\l@youtxciv,\l@youtycii){\framebox(\l@youthpvii,\l@youtvdii){}}
      \put(\l@youtxciv,\l@youtyciii){\framebox(\l@youthpvii,\l@youtvdii)%
        {\footnotesize\shortstack{Margin\\Note}}}
    \else
      \put(\l@youtxciv,\l@youtycii){\framebox(\l@youthpvii,\l@youtvdii)%
        {\footnotesize Note}}
      \put(\l@youtxciv,\l@youtyciii){\framebox(\l@youthpvii,\l@youtvdii)%
        {\footnotesize Margin}}
    \fi
    \ifdrawparameters
      \l@youtxcv=\l@youtxcii                % X coord for vertical dimensions
        \advance\l@youtxcv by \l@youthdii
      \l@youtvpii=\l@youthpvii              % X coord for marginparpush
        \divide\l@youtvpii by \tw@
        \advance\l@youtvpii by \l@youtxciv
      \l@youtvpvii=\l@youtvdiii             % half l@youtvdiii
        \divide\l@youtvpvii by \tw@
      \l@youtvpiii=\l@youtycii              % Y coord for low dimensions
        \advance\l@youtvpiii by \l@youtvdiii
      \l@youtvpiv=\l@youtvpiii              % Y coord for mid dimensions
        \advance\l@youtvpiv by \l@youtvdiii
      \l@youtvdi=\l@youtyciii               % Y coord for top dimensions
        \advance\l@youtvdi by \l@youtvdii
        \advance\l@youtvdi by -\l@youtvdiii
      \put(\l@youtxcv,\l@youtycv){\vector(0,-1){\l@youtvdiii}}
      \put(\l@youtxcv,\l@youtycv){\begin{picture}(\l@youtvpvii,\l@youtvpvii)
        \put(\l@youtvpvii,-\l@youtvpvii){\l@ypcmd{topmargin}}
        \end{picture}}
      \put(\l@youtxcv,\l@youtyciv){\line(0,1){\l@youtvdiii}}
      \put(\l@youtxcv,\l@youtyciv){\vector(0,-1){0}}
      \put(\l@youtxcv,\l@youtyciv){\begin{picture}(\l@youtvpvii,\l@youtvpvii)
        \put(\l@youtvpvii,\l@youtvpvii){\l@ypcmd{headheight}}
        \end{picture}}
      \put(\l@youtxcv,\l@youtyciv){\vector(0,-1){\l@youtvdiii}}
      \put(\l@youtxcv,\l@youtyciv){\begin{picture}(\l@youtvpvii,\l@youtvpvii)
        \put(\l@youtvpvii,-\l@youtvpvii){\l@ypcmd{headsep}}
        \end{picture}}
      \put(\l@youtxcv,\l@youtycii){\line(0,1){\l@youtvpv}}
      \put(\l@youtxcv,\l@youtycii){\vector(0,-1){0}}
      \put(\l@youtxcv,\l@youtycii){\begin{picture}(\l@youtvpvii,\l@youtvpvii)
        \put(\l@youtvpvii,\l@youtvpvii){\l@ypcmd{textheight}}
        \end{picture}}
      \put(\l@youtxcv,\l@youtycii){\vector(0,-1){\l@youtvpvi}}
      \put(\l@youtxcv,\l@youtycii){\begin{picture}(\l@youtvpvii,\l@youtvpvii)
        \put(\l@youtvpvii,-\l@youtvpvii){\l@ypcmd{footskip}}
        \end{picture}}
      \put(\l@youtvpii,\l@youtyciii){\vector(0,-1){\l@youtvdiii}}
      \put(\l@youtvpii,\l@youtyciii){\begin{picture}(\l@youtvpvii,\l@youtvpvii)
        \put(\l@youtvpvii,-\l@youtvpvii){\l@ypcmd{marginparpush}}
        \end{picture}}
      \put(\l@youtxciv,\l@youtvpiii){\vector(1,0){\l@youthpvii}}
      \put(\l@youtxciv,\l@youtvpiii){\begin{picture}(\l@youtvpvii,\l@youtvpvii)
        \put(\l@youtvpvii,\l@youtvpvii){\l@ypcmd{marginparwidth}}
        \end{picture}}
      \ifoddpagelayout
        \put(\l@youtxci,\l@youtvpiv){\vector(1,0){\l@youthdii}}
        \put(\l@youtxci,\l@youtvpiv){\begin{picture}(\l@youtvpvii,\l@youtvpvii)
          \put(\l@youtvpvii,\l@youtvpvii){\l@ypcmd{oddsidemargin}}
          \end{picture}}
        \put(\l@youtxciv,\l@youtvpiv){\line(-1,0){\l@youthdii}}
        \put(\l@youtxciv,\l@youtvpiv){\vector(1,0){0}}
        \put(\l@youtxciv,\l@youtvpiv){\begin{picture}(\l@youtvpvii,\l@youtvpvii)
          \put(-\l@youtvpvii,\l@youtvpvii){\l@ypcmd{marginparsep}}
          \end{picture}}
      \else
        \put(\l@youtxci,\l@youtvpiv){\vector(1,0){\l@youthdii}}
        \put(\l@youtxci,\l@youtvpiv){\begin{picture}(\l@youtvpvii,\l@youtvpvii)
          \put(\l@youtvpvii,\l@youtvpvii){\l@ypcmd{evensidemargin}}
          \end{picture}}
        \put(\l@youtxcii,\l@youtvdi){\vector(-1,0){\l@youthdii}}
        \put(\l@youtxcii,\l@youtvdi){\begin{picture}(\l@youtvpvii,\l@youtvpvii)
          \put(-\l@youtvpvii,\l@youtvpvii){\l@ypcmd{marginparsep}}
          \end{picture}}
      \fi
      \put(\l@youtxcii,\l@youtyciii){\vector(1,0){\l@youthpii}}
      \put(\l@youtxcv,\l@youtyciii){\begin{picture}(\l@youtvpvii,\l@youtvpvii)
        \put(\l@youtvpvii,\l@youtvpvii){\l@ypcmd{textwidth}}
        \end{picture}}
      \iftwocolumnlayout
        \put(\l@youtxciii,\l@youtvpiv){\line(-1,0){\l@youthpiii}}
        \put(\l@youtxciii,\l@youtvpiv){\vector(1,0){0}}
        \put(\l@youtxciii,\l@youtvpiv){\begin{picture}(\l@youtvpvii,\l@youtvpvii)
          \put(-\l@youtvpvii,\l@youtvpvii){\l@ypcmd{columnsep}}
          \end{picture}}
      \fi
    \fi
    \end{picture}
    \end{center}
    \setlength{\unitlength}{1pt}
    \ifdrawparameters\else
