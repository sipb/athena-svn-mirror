<!-- Remeber to increment SCHEMA_AGE in configure.in if you add any new
     schema to this file -->
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/gnomemeeting/devices/audio/plugin</key>
      <applyto>/apps/gnomemeeting/devices/audio/plugin</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>ALSA</default>
      <locale name="C">
	<short>Audio plugin</short>
	<long>The audio plugin that will be used to detect the devices and manage them</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/devices/audio/output_device</key>
      <applyto>/apps/gnomemeeting/devices/audio/output_device</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>None</default>
      <locale name="C">
	<short>Audio output device</short>
	<long>Select the audio output device to use</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/devices/audio/input_device</key>
      <applyto>/apps/gnomemeeting/devices/audio/input_device</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>None</default>
      <locale name="C">
	<short>Audio input device</short>
	<long>Select the audio input device to use</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/devices/video/plugin</key>
      <applyto>/apps/gnomemeeting/devices/video/plugin</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>V4L</default>
      <locale name="C">
	<short>Video plugin</short>
	<long>The video plugin that will be used to detect the devices and manage them</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/devices/video/input_device</key>
      <applyto>/apps/gnomemeeting/devices/video/input_device</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>/dev/video0</default>
      <locale name="C">
	<short>Video input device</short>
	<long>Select the video input device to use. If an error occurs when using this device a test picture will be transmitted.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/devices/video/size</key>
      <applyto>/apps/gnomemeeting/devices/video/size</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
	<short>Video size</short>
	<long>Select the transmitted video size: Small (QCIF 176x144) or Large (CIF 352x288)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/devices/video/channel</key>
      <applyto>/apps/gnomemeeting/devices/video/channel</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
	<short>Video channel</short>
	<long>The video channel number to use (to select camera, tv or other sources)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/devices/video/format</key>
      <applyto>/apps/gnomemeeting/devices/video/format</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
	<short>Video format</short>
	<long>Select the format for video cameras (does not apply to most USB cameras)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/devices/video/enable_preview</key>
      <applyto>/apps/gnomemeeting/devices/video/enable_preview</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>0</default>
      <locale name="C">
	<short>Video preview</short>
	<long>Video preview</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/devices/video/image</key>
      <applyto>/apps/gnomemeeting/devices/video/image</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>Video image</short>
	<long>The image to transmit if "Picture" is selected as video plugin or if the opening of the device fails. Leave blank to use the default GnomeMeeting logo.</long>
      </locale>
    </schema>
    <schema>	
      <key>/schemas/apps/gnomemeeting/devices/audio/lid_output_device_type</key>
      <applyto>/apps/gnomemeeting/devices/audio/lid_output_device_type</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
	<short>Output device type</short>
	<long>The output device type is the type of device connected to your Quicknet card. It can be either a POTS (Plain Old Telephone System) or a headset.</long>
      </locale>
    </schema>
    <schema>	
      <key>/schemas/apps/gnomemeeting/devices/audio/lid_echo_cancellation_level</key>
      <applyto>/apps/gnomemeeting/devices/audio/lid_echo_cancellation_level</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>2</default>
      <locale name="C">
	<short>Automatic echo cancellation</short>
	<long>The Automatic Echo Cancellation level: Off, Low, Medium, High, Automatic Gain Compensation. Choosing Automatic Gain Compensation modulates the volume for best quality.</long>
      </locale>
    </schema>
    <schema>	
      <key>/schemas/apps/gnomemeeting/devices/audio/lid_country_code</key>
      <applyto>/apps/gnomemeeting/devices/audio/lid_country_code</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>BE</default>
      <locale name="C">
	<short>Country code</short>
	<long>The two-letter country code of your country (e.g.: BE, UK, FR, DE, ...)</long>
      </locale>
    </schema>
    <schema>	
      <key>/schemas/apps/gnomemeeting/general/personal_data/firstname</key>
      <applyto>/apps/gnomemeeting/general/personal_data/firstname</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>First name</short>
	<long>Enter your first name</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/personal_data/lastname</key>
      <applyto>/apps/gnomemeeting/general/personal_data/lastname</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>Last name</short>
	<long>Enter your last name</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/personal_data/comment</key>
      <applyto>/apps/gnomemeeting/general/personal_data/comment</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>Comment</short>
	<long>Enter a comment about yourself for the user directory</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/personal_data/location</key>
      <applyto>/apps/gnomemeeting/general/personal_data/location</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>Location</short>
	<long>Enter your location (country or city) for the user directory</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/personal_data/mail</key>
      <applyto>/apps/gnomemeeting/general/personal_data/mail</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>E-mail address</short>
	<long>Enter your e-mail address</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/codecs/audio/list</key>
      <applyto>/apps/gnomemeeting/codecs/audio/list</applyto>
      <owner>GnomeMeeting</owner>
      <type>list</type>
      <list_type>string</list_type>
      <default>[iLBC-13k3=1,MS-GSM=1,SpeexNarrow-15k=1,iLBC-15k2=1,GSM-06.10=1,SpeexNarrow-8k=1,G.726-32k=1,G.711-uLaw-64k=1,G.711-ALaw-64k=1,G.723.1=0]</default>
      <locale name="C">
	<short>The Audio Codecs List</short>
	<long>The Audio Codecs List</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/codecs/audio/gsm_frames</key>
      <applyto>/apps/gnomemeeting/codecs/audio/gsm_frames</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>4</default>
      <locale name="C">
	<short>Number of frames for GSM</short>
	<long>Number of frames transmitted in each packet for the GSM codec</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/codecs/audio/g711_frames</key>
      <applyto>/apps/gnomemeeting/codecs/audio/g711_frames</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>30</default>
      <locale name="C">
	<short>Number of frames for G.711</short>
	<long>Number of frames transmitted in each packet for the G.711 codec</long>
      </locale>
    </schema>
    <schema>	
      <key>/schemas/apps/gnomemeeting/codecs/audio/enable_silence_detection</key>
      <applyto>/apps/gnomemeeting/codecs/audio/enable_silence_detection</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
	<short>Enable silence detection</short>
	<long>If enabled, use silence detection with the GSM and G.711 codecs</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/sound_events/incoming_call_sound</key>
      <applyto>/apps/gnomemeeting/general/sound_events/incoming_call_sound</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>gnomemeeting.wav</default>
      <locale name="C">
	<short>The incoming call sound</short>
	<long>The sound that will be played on incoming calls, if enabled</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/sound_events/enable_incoming_call_sound</key>
      <applyto>/apps/gnomemeeting/general/sound_events/enable_incoming_call_sound</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
	<short>Play sound on incoming calls</short>
	<long>The chosen sound will be played on incoming calls, if enabled</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/sound_events/incoming_call_sound</key>
      <applyto>/apps/gnomemeeting/general/sound_events/incoming_call_sound</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>gnomemeeting.wav</default>
      <locale name="C">
	<short>The incoming call sound</short>
	<long>The sound that will be played on incoming calls, if enabled</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/sound_events/enable_ring_tone_sound</key>
      <applyto>/apps/gnomemeeting/general/sound_events/enable_ring_tone_sound</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
	<short>Play ring tone</short>
	<long>The chosen sound will be played when calling somebody, if enabled</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/sound_events/ring_tone_sound</key>
      <applyto>/apps/gnomemeeting/general/sound_events/ring_tone_sound</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>ringtone.wav</default>
      <locale name="C">
	<short>The ring tone sound</short>
	<long>The sound that will be played when calling somebody, if enabled</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/sound_events/enable_busy_tone_sound</key>
      <applyto>/apps/gnomemeeting/general/sound_events/enable_busy_tone_sound</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
	<short>Play busy tone</short>
	<long>The chosen sound will be played at the end of calls or calling somebody who is busy, if enabled</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/sound_events/busy_tone_sound</key>
      <applyto>/apps/gnomemeeting/general/sound_events/busy_tone_sound</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>busytone.wav</default>
      <locale name="C">
	<short>The busy tone sound</short>
	<long>The sound that will be played at the end of calls or when calling somebody who is busy, if enabled</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/save_prefix</key>
      <applyto>/apps/gnomemeeting/general/save_prefix</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>gnomemeeting-snap-</default>
      <locale name="C">
	<short>The save prefix</short>
	<long>The prefix that will be used to save pictures of the conversation. That prefix is followed by the snapshot number.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/version</key>
      <applyto>/apps/gnomemeeting/general/version</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>9</default>
      <locale name="C">
	<short>Version</short>
	<long>Version</long>
      </locale>
    </schema>
    <schema>	
      <key>/schemas/apps/gnomemeeting/general/gconf_test_age</key>
      <applyto>/apps/gnomemeeting/general/gconf_test_age</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>@SCHEMA_AGE@</default>
      <locale name="C">
	<short>Revision of the schema file</short>
	<long>Revision of the schema file</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/ports/listen_port</key>
      <applyto>/apps/gnomemeeting/protocols/h323/ports/listen_port</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>1720</default>
      <locale name="C">
	<short>Listen port</short>
	<long>The port to listen to for incoming connections. GnomeMeeting needs to be restarted for the new value to take effect.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/ports/tcp_port_range</key>
      <applyto>/apps/gnomemeeting/protocols/h323/ports/tcp_port_range</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>30000:30010</default>
      <locale name="C">
	<short>TCP port range</short>
	<long>The range of TCP ports beside the listen_port that GnomeMeeting will use for the H.245 channel.  You can change the TCP port range if you are using IP translation and if you need to have several GnomeMeeting running behind your NAT/PAT router. GnomeMeeting needs to be restarted for the new values to take effect. This port range has no effect if both participants to the conference are using H.245 Tunneling.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/ports/rtp_port_range</key>
      <applyto>/apps/gnomemeeting/protocols/h323/ports/rtp_port_range</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>5000:5007</default>
      <locale name="C">
	<short>UDP port range</short>
	<long>The range of UDP ports that GnomeMeeting will use for RTP (audio and video communication channels). You can change the UDP port range if you are using IP translation and if you need to have several GnomeMeeting running behind your NAT/PAT router. GnomeMeeting needs to be restarted for the new values to take effect.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/ports/udp_port_range</key>
      <applyto>/apps/gnomemeeting/protocols/h323/ports/udp_port_range</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>5010:5013</default>
      <locale name="C">
	<short>UDP port range</short>
	<long>The range of UDP ports that GnomeMeeting will use when registering to gatekeepers. You can change the UDP port range if you are using IP translation and if you need to have several GnomeMeeting running behind your NAT/PAT router. GnomeMeeting needs to be restarted for the new values to take effect. This port range has no effect if you are not registering to a gatekeeper.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/advanced/enable_h245_tunneling</key>
      <applyto>/apps/gnomemeeting/protocols/h323/advanced/enable_h245_tunneling</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
	<short>Enable H.245 tunnelling</short>
	<long>This enables H.245 Tunnelling mode. In H.245 Tunnelling mode H.245 messages are encapsulated into the the H.225 channel (port 1720). This saves one TCP connection during calls. H.245 Tunnelling was introduced in H.323v2 and Netmeeting does not support it. Using both Fast Start and H.245 Tunnelling can crash some versions of Netmeeting.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/advanced/enable_early_h245</key>
      <applyto>/apps/gnomemeeting/protocols/h323/advanced/enable_early_h245</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
	<short>Enable early H.245</short>
	<long>This enables H.245 early in the setup</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/advanced/enable_fast_start</key>
      <applyto>/apps/gnomemeeting/protocols/h323/advanced/enable_fast_start</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
	<short>Enable 'Fast Start'</short>
	<long>Connection will be established in Fast Start mode. Fast Start is a new way to start calls faster that was introduced in H.323v2. It is not supported by Netmeeting and using both Fast Start and H.245 Tunnelling can crash some versions of Netmeeting</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/advanced/dtmf_sending</key>
      <applyto>/apps/gnomemeeting/protocols/h323/advanced/dtmf_sending</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
	<short>DTMF sending</short>
	<long>This permits to set the mode for DTMFs sending. The values can be "All" (0), "None" (1), "rfc2833" (2), "Signal" (3) or "String" (4) (default is "All"). Choosing other values than "All", "String" or "rfc2833" disables the Text Chat</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/gatekeeper/host</key>
      <applyto>/apps/gnomemeeting/protocols/h323/gatekeeper/host</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>Gatekeeper host</short>
	<long>The Gatekeeper host to register with</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/gatekeeper/password</key>
      <applyto>/apps/gnomemeeting/protocols/h323/gatekeeper/password</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>Gatekeeper password</short>
	<long>The Gatekeeper password to use for H.235 authentication to the Gatekeeper</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/gatekeeper/id</key>
      <applyto>/apps/gnomemeeting/protocols/h323/gatekeeper/id</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>Gatekeeper ID</short>
	<long>The Gatekeeper identifier to register with</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/gatekeeper/registering_method</key>
      <applyto>/apps/gnomemeeting/protocols/h323/gatekeeper/registering_method</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
	<short>Registering method</short>
	<long>The registering method to use</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/gatekeeper/alias</key>
      <applyto>/apps/gnomemeeting/protocols/h323/gatekeeper/alias</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>Gatekeeper alias</short>
	<long>The Gatekeeper alias to use when registering (string, or E164 ID if only 0123456789#)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/gatekeeper/register_alias_as_primary</key>
      <applyto>/apps/gnomemeeting/protocols/h323/gatekeeper/register_alias_as_primary</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
	<short>Gatekeeper alias as primary alias</short>
	<long>Use this option to ensure the above alias is used as the primary alias when registering with a gatekeeper. This may be required if your gatekeeper can only perform authentication using the first alias in the list</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/gateway/host</key>
      <applyto>/apps/gnomemeeting/protocols/h323/gateway/host</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>Gateway/proxy host</short>
	<long>The Gateway host is the host to use to do H.323 calls through a gateway that will relay calls</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/gateway/use_gateway</key>
      <applyto>/apps/gnomemeeting/protocols/h323/gateway/use_gateway</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
	<short>Use the gateway</short>
	<long>Use the specified gateway to do calls</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/call_forwarding/forward_host</key>
      <applyto>/apps/gnomemeeting/protocols/h323/call_forwarding/forward_host</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>h323:</default>
      <locale name="C">
	<short>Forward calls to host</short>
	<long>The host where calls should be forwarded to in the cases selected above</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/protocols/h323/call_forwarding/always_forward</key>
      <applyto>/apps/gnomemeeting/protocols/h323/call_forwarding/always_forward</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>false</default>
