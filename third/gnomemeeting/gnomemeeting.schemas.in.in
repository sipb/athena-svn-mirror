      <applyto>/apps/gnomemeeting/codecs/video/transmitted_background_blocks</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>2</default>
      <locale name="C">
	<short>Transmitted background blocks</short>
	<long>Choose the number of blocks (that have not changed) transmitted with each frame. These blocks fill in the background</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/codecs/video/transmitted_fps</key>
      <applyto>/apps/gnomemeeting/codecs/video/transmitted_fps</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>30</default>
      <locale name="C">
	<short>Minimum transmitted FPS</short>
	<long>The minimum number of video frames to transmit each second when trying to minimize the bandwidth</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/codecs/video/transmitted_video_quality</key>
      <applyto>/apps/gnomemeeting/codecs/video/transmitted_video_quality</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>75</default>
      <locale name="C">
	<short>Minimum transmitted video quality</short>
	<long>The minimum transmitted video quality to keep when trying to minimize the used bandwidth:  choose 100% on a LAN for the best quality, 1% being the worst quality</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/contacts/groups_list</key>
      <applyto>/apps/gnomemeeting/contacts/groups_list</applyto>
      <owner>GnomeMeeting</owner>
      <type>list</type>
      <list_type>string</list_type>
      <default>[Friends,Family,Work]</default>
      <locale name="C">
	<short>The list of groups</short>
	<long>The list of groups</long>
      </locale>
    </schema>
    <schema>		
      <key>/schemas/apps/gnomemeeting/contacts/ldap_servers_list</key>
      <applyto>/apps/gnomemeeting/contacts/ldap_servers_list</applyto>
      <owner>GnomeMeeting</owner>
      <type>list</type>
      <list_type>string</list_type>
      <default>[ils.seconix.com]</default>
      <locale name="C">
	<short>User directory</short>
	<long></long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/kind_of_net</key>
      <applyto>/apps/gnomemeeting/general/kind_of_net</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
	<short>Kind of network selected in the druid</short>
	<long>1: 56Kbps, 2: ISDN, 3:DSL, 4: LAN, 5: Custom</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/call_options/clear_inactive_calls</key>
      <applyto>/apps/gnomemeeting/general/call_options/clear_inactive_calls</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
	<short>Clear inactive calls</short>
	<long>If enabled, calls for which no audio and video has been received in the last 30 seconds are automatically cleared</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/call_options/no_answer_timeout</key>
      <applyto>/apps/gnomemeeting/general/call_options/no_answer_timeout</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>45</default>
      <locale name="C">
	<short>No answer timeout</short>
	<long>Automatically reject or forward incoming calls if no answer is given after the specified amount of time (in seconds)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/call_options/incoming_call_mode</key>
      <applyto>/apps/gnomemeeting/general/call_options/incoming_call_mode</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
	<short>Incoming call mode</short>
	<long>The way to react to incoming calls: 0 displays a popup, 1 automatically answers the call, 2 automatically rejects the call and 3 forwards the call to the given URL, if any</long>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>
