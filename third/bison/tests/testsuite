egrep -v '^ *\+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr >&5 || at_failed=:
$at_diff $at_devnull $at_stdout >&5 || at_failed=:
case $at_status in
   77) exit 77;;
   0) ;;
   *) $at_verbose "calc.at:387: exit code was $at_status, expected 0" >&2
      at_failed=:;;
esac

$at_failed && exit 1
