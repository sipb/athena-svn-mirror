msgstr "<b>Informace</b>"

#: cdburn.glade.h:5
msgid "<b>Write Options</b>"
msgstr "<b>Možnosti zápisu</b>"

#: cdburn.glade.h:6
msgid "<big><b>Writing files to disc progress</b></big>"
msgstr "<big><b>Průběh zápisu souborů na disk/b></big>"

#: cdburn.glade.h:7
msgid "<i>Initializing...</i>"
msgstr "<i>Inicializuji...</i>"

