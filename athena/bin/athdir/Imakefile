ATHSYS=`fs sysname | awk -F\' '{ print $$2 }'`

ATHDIR		=	athdir

OBJS		=	athdir.o

CFILES		=	athdir.c

LIBS		=

DEFINES		=	-DATHSYS=\"${ATHSYS}\"

AnsiCC()

SimpleProgram(${ATHDIR},${OBJS},${LIBS},${ATHBINDIR}/${ATHDIR})
install_man(${ATHDIR}.man,${ATHDIR}.1)

create_depend(${CFILES})
